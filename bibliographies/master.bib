	keywords = {Orbital debris, Spacecraft shield, Hypervelocity impact, Bumper materials, Protective mechanism},
	title = {Review of bumper materials for spacecraft shield against orbital debris hypervelocity impact},
	url = {https://www.sciencedirect.com/science/article/pii/S2214914724002186},
	year = {2024},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S2214914724002186},
