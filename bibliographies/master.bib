	journal = {Acta Astronautica},
	keywords = {Celestial mechanics, Sun, Jupiter, Lagrange points, Greeks, Trojans, Hilda, Thule, Asteroids, Mean motion resonance},
	pages = {208-215},
	title = {Jupiter, the great celestial organizer: On the origin of orbits of some asteroid families in mean motion resonances with Jupiter},
	url = {https://www.sciencedirect.com/science/article/pii/S009457652300293X},
	volume = {211},
	year = {2023},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S009457652300293X},
	bdsk-url-2 = {https://doi.org/10.1016/j.actaastro.2023.06.005}}

