	issn = {2405-8440},
	journal = {Heliyon},
	keywords = {Satellite orbit control, LQR controller, Infinite horizon, Discrete delay systems, Predictive controller, Reduction method},
	number = {2},
	pages = {e24265},
	title = {Design of infinite horizon LQR controller for discrete delay systems in satellite orbit control: A predictive controller and reduction method approach},
	url = {https://www.sciencedirect.com/science/article/pii/S2405844024002962},
	volume = {10},
