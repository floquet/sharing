	doi = {10.3390/app12178824},
	issn = {2076-3417},
	journal = {Applied Sciences},
	month = {9},
	number = 17,
	place = {United States},
	title = {Numerical Analysis of Blast Effects and Mitigation in the Far-Field from Small Explosions},
	url = {https://www.osti.gov/biblio/1902001},
