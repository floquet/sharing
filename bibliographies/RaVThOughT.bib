@inproceedings{moutet2024thrust,
	abstract = {
    This paper presents a prototype of a null deflection double linear axis thrust balance designed
    for 2D measurements of the vectorization of the thrust produced by electrical thrusters. The
    thrust measurement range spans from a few micro-Newtons to 10 milli-Newtons, with an
    accuracy of $\pm$50 micro-Newtons. Key features include scalable design, high repeatability,
    and the capability to validate thrust vectorization. This balance paves the way for a full
    3D thrust stand for comprehensive vectorization measurements.
  },
	address = {Pierre Baudis Convention Center, Toulouse, France},
	author = {P. Moutet and M. Zurkaulen and P. Thebault},
	booktitle = {38th International Electric Propulsion Conference},
	date-added = {2024-12-22 09:48:56 -0700},
	date-modified = {2024-12-22 09:48:56 -0700},
	keywords = {electric propulsion, vectorized thrust, 2D thrust balance, precision measurements},
	month = {June},
	note = {Copyright 2024 by the Electric Rocket Propulsion Society. All rights reserved.},
	title = {Overview of a 2D Thrust Balance Used for Accurate Measurements of Vectorized Electrical Thrusters},
	year = {2024}}

