@techreport{king1971thrust,
	abstract = {
    This report explores thrust vectoring techniques for a 5 cm mercury
    bombardment ion thruster. Key findings include the evaluation of 
    three vectoring systems: dual grid electrostatic, movable screen 
    electrode, and vectorable discharge chamber. Performance was 
    experimentally documented and analytically compared, with the 
    dual grid system demonstrating the most promise due to its 
    responsiveness and lack of moving parts. The report highlights 
    the challenges of ion beam deflection, including iterative 
    computational solutions and mechanical design trade-offs, while 
    proposing scalable approaches for future applications.
  },
	address = {Malibu, California},
	author = {H. J. King and C. R. Collett and D. E. Schnelker},
	date-added = {2024-12-22 10:12:59 -0700},
	date-modified = {2024-12-22 10:12:59 -0700},
	institution = {Hughes Research Laboratories},
	keywords = {Thrust vectoring, Ion thrusters, Electrostatic deflection, Spacecraft propulsion},
	number = {NAS 3-14058},
	title = {Thrust Vectoring Systems: Part 1--5 cm Systems},
	url = {https://ntrs.nasa.gov/api/citations/19710016808/downloads/19710016808.pdf},
	year = {1971},
