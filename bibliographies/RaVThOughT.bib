@techreport{farquhar1968libration,
	abstract = {This study provides a comprehensive analysis of satellite station-keeping in the vicinity of unstable collinear libration points L1 and L2. It includes simple linear feedback control laws, stability conditions for both constant and periodic coefficient systems, and station-keeping costs as functions of measurement noise. Applications for libration points are discussed, including lunar communications, interplanetary transportation, and deep-space optical communication. The research also explores solar-sail control and limit-cycle behavior for on-off control systems.},
	author = {Robert W. Farquhar},
