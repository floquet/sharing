@inproceedings{hunter1999counterflow,
	abstract = {
    A computational study of fluidic counterflow thrust vectoring was conducted, utilizing the PAB3D computational fluid dynamics code with two-equation turbulence closure and Reynolds stress modeling. The study validated computational results against experimental data and explored the physics of counterflow-induced thrust vectoring. It was revealed that effective vectoring can be achieved with less than 1\% secondary flow requirements. Insights into shear layer dynamics and pressure gradients were presented, offering implications for improved control in multi-functional propulsion systems.
  },
	address = {Los Angeles, CA},
	author = {Craig A. Hunter and Karen A. Deere},
	booktitle = {35th AIAA/ASME/SAE/ASEE Joint Propulsion Conference \& Exhibit},
