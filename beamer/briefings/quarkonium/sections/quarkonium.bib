	journal = {Annals of Physics},
	keywords = {Quantum chromodynamics, Chiral anomalies, Gauge field topology},
	number = {9},
	pages = {2349-2365},
	title = {The 't Hooft vertex revisited},
	url = {https://www.sciencedirect.com/science/article/pii/S0003491607001935},
	volume = {323},
	year = {2008},
